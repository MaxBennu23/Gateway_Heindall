@model IEnumerable<Gateway_Heindall.Models.Grupo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.GrupoName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GrupoDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GrupoArea)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GrupoType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GrupoMetodo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GrupoURL)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GrupoUser)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GrupoPassword)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GrupoPort)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PublicKey)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PrivateKey)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.GrupoName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GrupoDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GrupoArea)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GrupoType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GrupoMetodo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GrupoURL)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GrupoUser)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GrupoPassword)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GrupoPort)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PublicKey)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PrivateKey)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
